{
    "name": "cp-next",
    "displayName": "CP - The Next Generation",
    "description": "Syntax highlighting for the next-gen CP",
    "version": "0.2.0",
    "license": "Apache-2.0",
    "publisher": "yzyzsun",
    "repository": {
        "type": "git",
        "url": "https://github.com/yzyzsun/CP-next.git"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "cp",
            "aliases": ["CP"],
            "extensions": [".cp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cp",
            "scopeName": "source.cp",
            "path": "./CP.tmLanguage.json"
        }]
    }
}
