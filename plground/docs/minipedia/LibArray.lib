subArray T (a:[T]) (begin:Int) (end:Int) : [T] = 
  if begin < end then [a!!begin] ++ subArray @T a (begin+1) end
  else ([]:[T]);

head T (a:[T]) : T = a!!0;

tail T (a:[T]) : [T] = subArray @T a 1 (#a);

printArray' (a:[Int]) (i:Int) : String = 
  (if i==0 then "[" else "") ++ 
  (if i==(#a) then "]" else toString (a!!i) ++ "; " ++ printArray' a (i+1));
printArray (a:[Int]) = printArray' a 0;

